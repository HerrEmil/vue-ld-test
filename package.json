{
  "name": "vue-ld-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "launchdarkly-js-client-sdk": "^2.21.0",
    "vue": "^2.6.11",
    "vuex": "^3.6.2"
